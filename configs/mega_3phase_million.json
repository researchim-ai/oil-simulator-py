{
  "description": "3-phase IMPES, ~1e6 cells (100x100x100), 60 wells, PVT Black-Oil, substeps",
  "output_filename": "mega_3phase_million",
  "save_vtk": false,

  "simulation": {
    "solver_type": "impes",
    "total_time_days": 60,
    "time_step_days": 0.25,
    "save_interval": 10,
    "animation_fps": 2,
    "use_cuda": true,
    "max_substeps": 20,
    "global_rate_scale": 0.05,
    "cg_tolerance": 5e-6,
    "cg_max_iter": 8000,
    "pressure_float64": true,
    "pressure_solver": "amg",
    "amg": {
      "theta": 0.2,
      "max_levels": 20,
      "coarsest_size": 120,
      "tol": 1e-3,
      "max_cycles": 40
    }
  },

  "reservoir": {
    "dimensions": [100, 100, 100],
    "grid_size": [10.0, 10.0, 2.0],
    "porosity": 0.2,
    "permeability": 150.0,
    "k_vertical_fraction": 0.15
  },

